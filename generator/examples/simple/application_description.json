{
  "settings": {
    "development": true
  },
  "cluster_latencies": [
    {
      "src" : "cluster1",
      "dest": "cluster2",
      "latency": 0.002
    }
  ],
  "services": [
    {
      "name": "service1",
      "clusters": [
        {
          "cluster": "cluster1",
          "namespace": "default",
          "node": "cluster1-control-plane"
        }
      ],
      "resources": {
        "limits": {
          "memory": "1024M",
          "cpu": "2000m"
        },
        "requests": {
          "memory": "1024M",
          "cpu": "2000m"
        }
      },
      "processes": 2,
      "readiness_probe": 1,
      "endpoints": [
        {
          "name": "endpoint1",
          "protocol": "http",
          "execution_mode": "sequential",
          "network_complexity": {
            "forward_requests": "asynchronous",
            "response_payload_size": 512,
            "called_services": [
              {
                "service": "service2",
                "port": 80,
                "endpoint": "endpoint1",
                "protocol": "http",
                "traffic_forward_ratio": 1,
                "request_payload_size": 256
              },
              {
                "service": "service2",
                "port": 80,
                "endpoint": "endpoint2",
                "protocol": "http",
                "traffic_forward_ratio": 1,
                "request_payload_size": 256
              }
            ]
          }
        },
        {
          "name": "endpoint2",
          "protocol": "http",
          "execution_mode": "parallel",
          "network_complexity": {
            "forward_requests": "asynchronous",
            "response_payload_size": 512
          }
        }
      ]
    },
    {
      "name": "service2",
      "clusters": [
        {
          "cluster": "cluster1",
          "namespace": "default",
          "node": "cluster1-control-plane"
        }
      ],
      "resources": {
        "limits": {
          "memory": "1024M",
          "cpu": "2000m"
        },
        "requests": {
          "memory": "1024M",
          "cpu": "2000m"
        }
      },
      "processes": 2,
      "readiness_probe": 1,
      "endpoints": [
        {
          "name": "endpoint1",
          "protocol": "http",
          "execution_mode": "parallel",
          "cpu_complexity": {
            "execution_time": 1,
            "threads": 2
          },
          "network_complexity": {
            "forward_requests": "asynchronous",
            "response_payload_size": 512
          }
        },
        {
          "name": "endpoint2",
          "protocol": "http",
          "execution_mode": "parallel",
          "network_complexity": {
            "forward_requests": "asynchronous",
            "response_payload_size": 512
          }
        }
      ]
    }
  ]
}