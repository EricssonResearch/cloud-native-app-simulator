{
  "cluster_latencies": [
    {
      "src" : "cluster-1",
      "dest": "cluster-2",
      "latency": 0.002
    }
  ],
  "services": [
    {
      "name": "service-1",
      "clusters": [
        {
          "cluster": "cluster-1",
          "namespace": "ns-1",
          "node": "node-1"
        }
      ],
      "resources": {
        "limits": {
          "memory": "1024M",
          "cpu": "1000m"
        },
        "requests": {
          "memory": "512M",
          "cpu": "500m"
        }
      },
      "processes": 2,
      "threads": 2,
      "readiness_probe": 5,
      "endpoints": [
        {
          "name": "/end-1",
          "protocol": "http",
          "cpu_consumption": 0.003,
          "network_consumption": 0.002,
          "memory_consumption": 0.003,
          "forward_requests": "asynchronous",
          "called_services": [
            {
              "service": "service-2",
              "port": "80",
              "endpoint": "/end-2",
              "protocol": "http",
              "traffic_forward_ratio": 1
            }
          ]
        }
      ]
    }
  ]
}