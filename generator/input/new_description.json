{
  "settings": {
    "logging": true
  },
  "cluster_latencies": [
    {
      "src": "cluster-1",
      "dest": "cluster-2",
      "latency": 0.002
    }
  ],
  "services": [
    {
      "name": "service1",
      "clusters": [
        {
          "cluster": "cluster-1",
          "replicas": 1,
          "namespace": "default",
          "node": "node-1"
        }
      ],
      "resources": {
        "limits": {
          "memory": "1024M",
          "cpu": "1000m"
        },
        "requests": {
          "memory": "512M",
          "cpu": "500m"
        }
      },
      "processes": 2,
      "threads": 2,
      "readiness_probe": 5,
      "endpoints": [
        {
          "name": "end1",
          "protocol": "http",
          "execution_mode": "sequential",
          "cpu_complexity": {
            "execution_time": "0.1s",
            "method": "fibonacci",
            "workers": 2,
            "cpu_affinity": [
              0,
              2
            ],
            "cpu_load": "10%"
          },
          "memory_complexity": {
            "execution_time": "0.1s",
            "method": "swap",
            "workers": 2,
            "bytes_load": "10%"
          },
          "network_complexity": {
            "forward_requests": "asynchronous",
            "response_payload_size": 256,
            "called_services": [
              {
                "service": "service2",
                "port": "80",
                "endpoint": "end2",
                "protocol": "http",
                "traffic_forward_ratio": 1,
                "request_payload_size": 512
              }
            ]
          }
        }
      ]
    }
  ]
}